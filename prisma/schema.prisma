// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model Warning {
  id       Int      @id @default(sequence())
  user_id  String
  guild_id String
  reason   String
  time     DateTime
}
